  <template name="nweb" value="require_once( 'config.php' );&#10;&#10;$END$&#10;&#10;&#10;$nav = [];&#10;include( KERNEL::render( $nav, [] ) );&#10;" description="New web file" toReformat="false" toShortenFQNames="true">
